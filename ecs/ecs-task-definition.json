{
    "containerDefinitions": [
        {
            "cpu": 0,
            "dependsOn": [
                {
                    "condition": "HEALTHY",
                    "containerName": "backend"
                },
                {
                    "condition": "HEALTHY",
                    "containerName": "db"
                }
            ],
            "environment": [],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:80/health || exit 1"
                ],
                "interval": 10,
                "retries": 5,
                "startPeriod": 20,
                "timeout": 5
            },
            "image": "905418191531.dkr.ecr.ap-southeast-1.amazonaws.com/nusiss-repository:frontend-latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs/fargate/commonqueue",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "frontend"
                }
            },
            "mountPoints": [],
            "name": "frontend",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        },
        {
            "cpu": 0,
            "dependsOn": [
                {
                    "condition": "HEALTHY",
                    "containerName": "db"
                }
            ],
            "environment": [],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "exit 0"
                ],
                "interval": 10,
                "retries": 5,
                "startPeriod": 20,
                "timeout": 5
            },
            "image": "905418191531.dkr.ecr.ap-southeast-1.amazonaws.com/nusiss-repository:backend-latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs/fargate/commonqueue",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "backend"
                }
            },
            "mountPoints": [],
            "name": "backend",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "secrets": [
                {
                    "name": "SPRING_DATASOURCE_USERNAME",
                    "valueFrom": "arn:aws:ssm:ap-southeast-1:905418191531:parameter/SPRING_DATASOURCE_USERNAME"
                },
                {
                    "name": "SPRING_DATASOURCE_PASSWORD",
                    "valueFrom": "arn:aws:ssm:ap-southeast-1:905418191531:parameter/SPRING_DATASOURCE_PASSWORD"
                },
                {
                    "name": "SPRING_DATASOURCE_URL",
                    "valueFrom": "arn:aws:ssm:ap-southeast-1:905418191531:parameter/SPRING_DATASOURCE_URL"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        },
        {
            "cpu": 0,
            "environment": [],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "mysqladmin ping -uroot -p$MYSQL_ROOT_PASSWORD"
                ],
                "interval": 30,
                "retries": 3,
                "startPeriod": 30,
                "timeout": 5
            },
            "image": "905418191531.dkr.ecr.ap-southeast-1.amazonaws.com/nusiss-repository:db-latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs/fargate/commonqueue",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "db"
                }
            },
            "mountPoints": [],
            "name": "db",
            "portMappings": [
                {
                    "containerPort": 3306,
                    "hostPort": 3306,
                    "protocol": "tcp"
                }
            ],
            "secrets": [
                {
                    "name": "MYSQL_ROOT_PASSWORD",
                    "valueFrom": "arn:aws:ssm:ap-southeast-1:905418191531:parameter/MYSQL_ROOT_PASSWORD"
                },
                {
                    "name": "MYSQL_DATABASE",
                    "valueFrom": "arn:aws:ssm:ap-southeast-1:905418191531:parameter/MYSQL_DATABASE"
                },
                {
                    "name": "MYSQL_USER",
                    "valueFrom": "arn:aws:ssm:ap-southeast-1:905418191531:parameter/MYSQL_USER"
                },
                {
                    "name": "MYSQL_PASSWORD",
                    "valueFrom": "arn:aws:ssm:ap-southeast-1:905418191531:parameter/MYSQL_PASSWORD"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        }
    ],
    "cpu": "1024",
    "executionRoleArn": "arn:aws:iam::905418191531:role/ecsTaskExecutionRole",
    "taskroleArn" : "arn:aws:iam::905418191531:role/nusiss_practiceproject",
    "family": "main-app-td",
    "memory": "2048",
    "networkMode": "awsvpc",
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "volumes": [],
    "tags": []
}